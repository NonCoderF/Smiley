<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleProgress">
        <attr name="circleGap" format="float"/>
        <attr name="startAngle" format="float">
            <flag name="left" value="-180"/>
            <flag name="top" value="-90"/>
            <flag name="right" value="0"/>
            <flag name="bottom" value="90"/>
        </attr>
        <attr name="textVisible" format="boolean"/>
        <attr name="textFont" format="reference"/>

    </declare-styleable>

</resources>